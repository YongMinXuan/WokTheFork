<!-- Winter Breeze -->
<!-- Common Base Styling -->
{% assign primary_font = "Helvetica Neue"%}
{% assign primary_font_color = "#000" %} <!-- Black text color -->
{% assign primary_font_size = "11px"%}
{% assign secondary_font = "Helvetica Neue"%}
{% assign secondary_font_color = "#444" %} <!-- Grey text color -->
{% assign theme_color = "#000000"%}
<script>
    $.get("https://fonts.googleapis.com/css?family={{ primary_font | replace: " ", "+" }}:300,400,700").done(function(response) {
          var pFontCss = response.replace(/ local\(.+?\)\,/g,"");
          var pFontCode = '<style id="font-primary-9977" type="text/css">' + pFontCss + '</style>'
          $('head').append(pFontCode);
        });
    {% if secondary_font != blank and secondary_font != primary_font %}
        $.get("https://fonts.googleapis.com/css?family={{ secondary_font | replace: " ", "+" }}:300,400,700").done(function(response) {
          var sFontCss = response.replace(/ local\(.+?\)\,/g,"");
          var sFontCode = '<style id="font-secondary-9977" type="text/css">' + sFontCss + '</style>'
          $('head').append(sFontCode);
        });
    {% endif %}
</script>
<script>
    $.ajaxSetup({
      cache: true
    });
    var url = "https://orderprintertemplates.com/JsBarcode.all.min.js";
    var options = {
      format: "CODE128",
      width: 1,
      height: 14,
      quite: 1,
      lineColor: "{{ primary_font_color }}",
      displayValue: false, //Show the text below the barcode (can be true or false)
      fontSize: {{ primary_font_size | remove: "px" | minus: 1 }},
      font: "{{ primary_font }}",
      textAlign: "left"
    }
    $.getScript( url, function() {
      // Barcode Generator for Order Number
      $(".barcode-{{ order_number }}").each(function(){
        $(this).JsBarcode("{{ order_name }}",options);
      });
      // Barcode Generator for Products
      $('.product-barcode').each(function(){
          var productBarcode = String($(this).data('barcode'));
          if (productBarcode != null || productBarcode != ''){
            $(this).JsBarcode(productBarcode, options);
          };
       });
    });
</script>
<script>
    $(function () {
         if (navigator.userAgent.indexOf('Safari') != -1 &&
         navigator.userAgent.indexOf('Chrome') == -1) {
